#!/bin/bash
# Ejemplo de cómo configurar credenciales de AWS para la aplicación
# Copia este archivo a start-with-credentials.sh y completa con tus credenciales reales

export AWS_PROFILE=tappy
export AWS_ACCESS_KEY_ID=your_access_key_id_here
export AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
export AWS_REGION=us-east-1

# Ejecutar DynamoDB con estas credenciales
cd "$(dirname "$0")"
node server.js
